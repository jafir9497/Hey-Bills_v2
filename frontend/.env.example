# =============================================================================
# Hey-Bills Flutter Frontend Environment Configuration
# =============================================================================
# Copy this file to .env and configure with your actual values
# This file is specifically for the Flutter frontend application

# =============================================================================
# SUPABASE CONFIGURATION (Required)
# =============================================================================

# Supabase Project Configuration
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here

# =============================================================================
# API ENDPOINTS
# =============================================================================

# Backend API Configuration  
API_BASE_URL=http://localhost:3001
WEBSOCKET_URL=ws://localhost:3001

# Alternative API endpoints for different environments
API_BASE_URL_DEV=http://localhost:3001
API_BASE_URL_STAGING=https://api-staging.heybills.com
API_BASE_URL_PROD=https://api.heybills.com

# =============================================================================
# OAUTH CONFIGURATION
# =============================================================================

# Google OAuth Client IDs for different platforms
GOOGLE_CLIENT_ID_WEB=your-google-web-client-id.googleusercontent.com
GOOGLE_CLIENT_ID_ANDROID=your-google-android-client-id.googleusercontent.com  
GOOGLE_CLIENT_ID_IOS=your-google-ios-client-id.googleusercontent.com

# =============================================================================
# APPLICATION CONFIGURATION
# =============================================================================

# App Information
APP_NAME=Hey Bills
PACKAGE_NAME=com.heybills.app
URL_SCHEME=heybills
APP_VERSION=1.0.0

# Environment Settings
FLUTTER_ENV=development
DEBUG_MODE=true
ENABLE_LOGGING=true

# =============================================================================
# FEATURES & FUNCTIONALITY
# =============================================================================

# Core Features
ENABLE_OCR=true
ENABLE_AI_CHAT=true
ENABLE_VECTOR_SEARCH=true
ENABLE_OFFLINE_MODE=true
ENABLE_BIOMETRIC_AUTH=true

# Camera & Media Features
ENABLE_CAMERA=true
ENABLE_GALLERY_IMPORT=true
ENABLE_FILE_IMPORT=true
MAX_IMAGE_SIZE_MB=10

# Notification Features
ENABLE_PUSH_NOTIFICATIONS=true
ENABLE_LOCAL_NOTIFICATIONS=true
ENABLE_WARRANTY_ALERTS=true

# Advanced Features
ENABLE_MULTI_CURRENCY=false
ENABLE_LOCATION_TRACKING=false
ENABLE_VOICE_NOTES=false
ENABLE_DARK_MODE=true
ENABLE_ACCESSIBILITY=true

# =============================================================================
# FIREBASE CONFIGURATION (for Push Notifications)
# =============================================================================

# Firebase Project Configuration
FIREBASE_PROJECT_ID=your-firebase-project-id
FIREBASE_APP_ID_ANDROID=1:123456789012:android:abcdef123456789
FIREBASE_APP_ID_IOS=1:123456789012:ios:abcdef123456789
FIREBASE_APP_ID_WEB=1:123456789012:web:abcdef123456789

# Firebase Cloud Messaging
FCM_SENDER_ID=123456789012
FCM_VAPID_KEY=your-firebase-vapid-key-for-web-notifications
FCM_SERVER_KEY=your-firebase-server-key

# Firebase Config Object (for web)
FIREBASE_API_KEY=AIzaSyAbCdEfGhIjKlMnOpQrStUvWxYz1234567
FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
FIREBASE_DATABASE_URL=https://your-project-id-default-rtdb.firebaseio.com
FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
FIREBASE_MESSAGING_SENDER_ID=123456789012
FIREBASE_MEASUREMENT_ID=G-ABCDEFGHIJ

# =============================================================================
# ANALYTICS & MONITORING
# =============================================================================

# Error Tracking
SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
SENTRY_ENVIRONMENT=development

# Analytics
ENABLE_ANALYTICS=true
GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
MIXPANEL_TOKEN=your-mixpanel-token

# Performance Monitoring
ENABLE_PERFORMANCE_MONITORING=true
FIREBASE_PERFORMANCE_ENABLED=true

# =============================================================================
# DEVELOPMENT & DEBUGGING
# =============================================================================

# Debug Configuration
ENABLE_INSPECTOR=true
ENABLE_WIDGET_INSPECTOR=true
LOG_LEVEL=debug
PRETTY_PRINT_LOGS=true

# Network Debugging
LOG_API_CALLS=true
LOG_WEBSOCKET_MESSAGES=true
MOCK_API_RESPONSES=false

# UI Debugging
SHOW_DEBUG_BANNER=true
SHOW_PERFORMANCE_OVERLAY=false
SHOW_SEMANTIC_DEBUGGER=false

# =============================================================================
# SECURITY CONFIGURATION
# =============================================================================

# Biometric Authentication
ENABLE_FINGERPRINT=true
ENABLE_FACE_ID=true
ENABLE_PIN_FALLBACK=true
SESSION_TIMEOUT_MINUTES=30

# Data Security
ENABLE_ENCRYPTION=true
ENCRYPTION_KEY_ALIAS=heybills_encryption_key
SECURE_STORAGE_PREFIX=heybills_secure_

# Certificate Pinning
ENABLE_CERTIFICATE_PINNING=true
API_CERTIFICATE_SHA256=your-api-cert-sha256-hash

# =============================================================================
# PERFORMANCE & CACHING
# =============================================================================

# Caching Configuration
ENABLE_HTTP_CACHE=true
HTTP_CACHE_SIZE_MB=50
IMAGE_CACHE_SIZE_MB=100
CACHE_DURATION_HOURS=24

# Network Configuration
CONNECTION_TIMEOUT_SECONDS=30
READ_TIMEOUT_SECONDS=30
MAX_CONCURRENT_REQUESTS=10

# Database Configuration
LOCAL_DB_VERSION=1
AUTO_BACKUP_ENABLED=true
BACKUP_FREQUENCY_HOURS=24

# =============================================================================
# UI/UX CONFIGURATION
# =============================================================================

# Theme Settings
DEFAULT_THEME=light
SUPPORT_DARK_MODE=true
SUPPORT_SYSTEM_THEME=true
PRIMARY_COLOR=0xFF2196F3
ACCENT_COLOR=0xFF03DAC6

# Localization
DEFAULT_LOCALE=en_US
SUPPORTED_LOCALES=en_US,es_ES,fr_FR
CURRENCY_SYMBOL=$
DATE_FORMAT=MM/dd/yyyy

# Accessibility
ENABLE_SCREEN_READER=true
MIN_TOUCH_TARGET_SIZE=44
HIGH_CONTRAST_MODE=false

# =============================================================================
# PLATFORM-SPECIFIC CONFIGURATION
# =============================================================================

# Android Configuration
ANDROID_MIN_SDK_VERSION=21
ANDROID_TARGET_SDK_VERSION=34
ANDROID_COMPILE_SDK_VERSION=34

# iOS Configuration
IOS_DEPLOYMENT_TARGET=12.0
IOS_TEAM_ID=your-ios-team-id
IOS_BUNDLE_ID=com.heybills.app

# Web Configuration (if supporting web)
WEB_RENDERER=html
ENABLE_PWA=true
PWA_MANIFEST_PATH=web/manifest.json

# =============================================================================
# TESTING CONFIGURATION
# =============================================================================

# Test Environment
TEST_SUPABASE_URL=https://test-project-ref.supabase.co
TEST_SUPABASE_ANON_KEY=test-anon-key
TEST_API_BASE_URL=http://localhost:3001
MOCK_SERVICES_IN_TESTS=true

# Integration Testing
ENABLE_INTEGRATION_TESTS=true
TEST_TIMEOUT_SECONDS=30
SCREENSHOT_ON_FAILURE=true

# =============================================================================
# BUILD CONFIGURATION
# =============================================================================

# Build Settings
BUILD_NUMBER=1
BUILD_MODE=debug
OBFUSCATE_CODE=false
TREE_SHAKE_ICONS=true

# Asset Configuration
COMPRESS_ASSETS=true
GENERATE_LOCALIZATION_FILES=true
INCLUDE_SOURCE_MAPS=false

# =============================================================================
# EXPERIMENTAL FEATURES
# =============================================================================

# Flutter Features
ENABLE_IMPELLER=true
ENABLE_MATERIAL_3=true
ENABLE_CUSTOM_FONTS=true

# Experimental APIs
ENABLE_WEB_ASSEMBLY=false
ENABLE_ISOLATES=true
ENABLE_BACKGROUND_PROCESSING=true

# =============================================================================
# AI & ML SERVICES
# =============================================================================

# OpenAI Configuration (for client-side if needed)
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_MODEL=gpt-4-turbo-preview

# OpenRouter Configuration (for client-side AI features)
OPENROUTER_API_KEY=sk-or-v1-your-openrouter-api-key-here
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
OPENROUTER_MODEL=anthropic/claude-3.5-sonnet
OPENROUTER_APP_NAME=Hey Bills
OPENROUTER_APP_URL=https://heybills.com

# =============================================================================
# THIRD-PARTY INTEGRATIONS
# =============================================================================

# Map Services (if needed)
GOOGLE_MAPS_API_KEY_ANDROID=your-android-maps-key
GOOGLE_MAPS_API_KEY_IOS=your-ios-maps-key
GOOGLE_MAPS_API_KEY_WEB=your-web-maps-key

# Payment Processing (future feature)
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_key
STRIPE_WEBHOOK_SECRET=whsec_your_stripe_webhook_secret
PAYPAL_CLIENT_ID=your-paypal-client-id

# Social Media Sharing
ENABLE_SOCIAL_SHARING=false
FACEBOOK_APP_ID=your-facebook-app-id
TWITTER_API_KEY=your-twitter-api-key
LINKEDIN_CLIENT_ID=your-linkedin-client-id

# =============================================================================
# ENVIRONMENT DETECTION
# =============================================================================

# Build Variants
IS_DEVELOPMENT=true
IS_STAGING=false
IS_PRODUCTION=false

# Feature Flags Service
FEATURE_FLAGS_ENABLED=false
LAUNCHDARKLY_SDK_KEY=your-launchdarkly-key

# =============================================================================
# DEPLOYMENT & BUILD CONFIGURATION
# =============================================================================

# Production/Staging URLs
PROD_API_BASE_URL=https://api.heybills.com
PROD_SUPABASE_URL=https://your-prod-project.supabase.co
PROD_SUPABASE_ANON_KEY=prod-anon-key

STAGING_API_BASE_URL=https://api-staging.heybills.com
STAGING_SUPABASE_URL=https://your-staging-project.supabase.co
STAGING_SUPABASE_ANON_KEY=staging-anon-key

# CDN Configuration
CDN_BASE_URL=https://cdn.heybills.com
STATIC_ASSETS_URL=https://assets.heybills.com

# Build Configuration
BUILD_TIMESTAMP=2024-01-01T00:00:00.000Z
BUILD_VERSION=1.0.0+1
BUILD_NUMBER=1

